#include <stdio.h>

int main() {
    int hours, minutes, total_minutes;
    
    // Prompt user for input
    printf("Enter hours: ");
    scanf("%d", &hours);
    
    printf("Enter minutes: ");
    scanf("%d", &minutes);
    
    // Calculate total minutes
    total_minutes = (hours * 60) + minutes;
    
    // Display result
    printf("Total minutes: %d\n", total_minutes);
    
    return 0;
}
