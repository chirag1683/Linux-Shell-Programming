int main() {
    int num1, num2;

    // Accept two integers from the user
    printf("Enter two integers: ");
    scanf("%d %d", &num1, &num2);

    // Check if the numbers are equal
    if (num1 == num2) {
        printf("The numbers are equal.\n");
    } else {
        printf("The numbers are not equal.\n");
    }

    return 0;
}
