#include <stdio.h>

int main() {
    int age;

    // Accept age from the user
    printf("Enter your age: ");
    scanf("%d", &age);

    // Check if eligible to vote
    if (age >= 18) {
        printf("You are eligible to vote.\n");
    } else {
        printf("You are not eligible to vote. You need to wait %d more years.\n", 18 - age);
    }

    return 0;
}
